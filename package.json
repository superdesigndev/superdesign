{
  "name": "securedesign",
  "displayName": "securedesign",
  "description": "The 1st Design Agent lives inside your IDE",
  "version": "0.0.14",
  "publisher": "HaroldMartin",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hbmartin/secure-design"
  },
  "engines": {
    "vscode": "^1.99.1"
  },
  "categories": [
    "AI",
    "Other"
  ],
  "keywords": [
    "design",
    "ui",
    "mockup",
    "wireframe",
    "component",
    "ai",
    "agent",
    "figma",
    "prototype",
    "layout",
    "frontend",
    "react",
    "html",
    "css",
    "claude",
    "anthropic",
    "generator",
    "automation"
  ],
  "activationEvents": [
    "onCommand:securedesign.openSettings",
    "onView:securedesign.chatView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "securedesign.configureApiKey",
        "title": "Configure Anthropic API Key",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.configureOpenAIApiKey",
        "title": "Configure OpenAI API Key",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.configureOpenAIUrl",
        "title": "Configure OpenAI url",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.configureOpenRouterApiKey",
        "title": "Configure OpenRouter API Key",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.configureGoogleApiKey",
        "title": "Configure Google API Key",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.configureAWSBedrock",
        "title": "Configure AWS Bedrock",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.configureMoonshotApiKey",
        "title": "Configure Moonshot API Key",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.showChatSidebar",
        "title": "Show Chat Sidebar",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.openCanvas",
        "title": "Open Canvas View",
        "category": "Securedesign",
        "icon": "$(window)"
      },
      {
        "command": "securedesign.clearChat",
        "title": "Clear Chat",
        "category": "Securedesign",
        "icon": "$(clear-all)"
      },
      {
        "command": "securedesign.resetWelcome",
        "title": "Reset Welcome Screen",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.initializeProject",
        "title": "Initialize Securedesign",
        "category": "Securedesign"
      },
      {
        "command": "securedesign.openSettings",
        "title": "Open Settings",
        "category": "Securedesign",
        "icon": "$(settings-gear)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "securedesign.openCanvas",
          "when": "view == securedesign.chatView",
          "group": "navigation"
        },
        {
          "command": "securedesign.openSettings",
          "when": "view == securedesign.chatView",
          "group": "navigation"
        },
        {
          "command": "securedesign.clearChat",
          "when": "view == securedesign.chatView",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "securedesign-sidebar": [
        {
          "type": "webview",
          "id": "securedesign.chatView",
          "name": "SUPER DESIGN",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "securedesign-sidebar",
          "title": "Securedesign",
          "icon": "icon.svg"
        }
      ]
    },
    "configuration": {
      "title": "Securedesign",
      "properties": {
        "securedesign.anthropicApiKey": {
          "type": "string",
          "description": "Anthropic API key for Claude Code integration",
          "scope": "application"
        },
        "securedesign.openaiApiKey": {
          "type": "string",
          "description": "OpenAI API key for custom agent",
          "scope": "application"
        },
        "securedesign.openaiUrl": {
          "type": "string",
          "description": "OpenAI url for custom agent",
          "scope": "application"
        },
        "securedesign.openrouterApiKey": {
          "type": "string",
          "description": "OpenRouter API key for custom agent",
          "scope": "application"
        },
        "securedesign.googleApiKey": {
          "type": "string",
          "description": "Google API key for Gemini models.",
          "scope": "application"
        },
        "securedesign.aiModelProvider": {
          "type": "string",
          "description": "AI model provider for custom agent",
          "scope": "application"
        },
        "securedesign.moonshotApiKey": {
          "type": "string",
          "description": "Moonshot AI API key for Kimi K2 integration",
          "scope": "application"
        },
        "securedesign.aiModel": {
          "type": "string",
          "description": "Specific AI model to use (e.g., gpt-4o, claude-3-5-sonnet-20241022)",
          "scope": "application"
        },
        "securedesign.awsAccessKeyId": {
          "type": "string",
          "description": "AWS Access Key ID for Bedrock integration",
          "scope": "application"
        },
        "securedesign.awsSecretAccessKey": {
          "type": "string",
          "description": "AWS Secret Access Key for Bedrock integration",
          "scope": "application"
        },
        "securedesign.awsRegion": {
          "type": "string",
          "default": "us-east-1",
          "description": "AWS region for Bedrock services",
          "scope": "application"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --quiet",
    "test": "vscode-test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "npm run lint -- --fix",
    "test:llm": "tsc --project tsconfig.test.json && node dist-test/test/llm-service.test.js",
    "test:core": "tsc --project tsconfig.test.json && node dist-test/test/core-components.test.js",
    "test:read": "tsc --project tsconfig.test.json && node dist-test/test/read-tool.test.js",
    "test:write-edit": "tsc --project tsconfig.test.json && node dist-test/test/write-edit-tools.test.js",
    "test:ls-grep-glob": "tsc --project tsconfig.test.json && node dist-test/test/ls-grep-glob-tools.test.js",
    "test:agent": "tsc --project tsconfig.test.json && node dist-test/test/llm-service.test.js && node dist-test/test/core-components.test.js",
    "test:tools": "tsc --project tsconfig.test.json && node dist-test/test/read-tool.test.js && node dist-test/test/write-edit-tools.test.js && node dist-test/test/ls-grep-glob-tools.test.js"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.17.0",
    "@types/vscode": "1.99.1",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.9",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.8",
    "@ai-sdk/anthropic": "^2.0.4",
    "@ai-sdk/google": "^2.0.6",
    "@ai-sdk/openai": "^2.0.15",
    "@anthropic-ai/claude-code": "^1.0.83",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "ai": "^5.0.15",
    "execa": "^9.6.0",
    "glob": "^11.0.3",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.539.0",
    "micromatch": "^4.0.8",
    "mime-types": "^3.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "zod": "^4.0.17"
  },
  "overrides": {
    "zod-to-json-schema": "npm:@alcyone-labs/zod-to-json-schema@latest"
  }
}
